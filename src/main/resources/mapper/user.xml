<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lynn.web.mapper.UserMapper">

    <select id="findUserByUserName" resultType="com.lynn.web.entities.User" parameterType="String">
        select * from `user` where username = #{username}#
    </select>

</mapper>